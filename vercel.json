{
  "crons": [
    {
      "path": "/api/cron/process-scheduled-messages?token=${CRON_SECRET_KEY}",
      "schedule": "*/15 * * * *"
    },
    {
      "path": "/api/cron/process-birthday-messages?token=${CRON_SECRET_KEY}",
      "schedule": "0 9 * * *"
    },
    {
      "path": "/api/cron/cleanup-stuck-messages?token=${CRON_SECRET_KEY}",
      "schedule": "0 */2 * * *"
    }
  ]
}